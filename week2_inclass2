package practice;

public class practice {

	public static void main(String[] args) {
		int x = (int)5.25;
		System.out.println("x = " + x); //outputs 'x = 5'
		double y = 5;
		System.out.println("y = " + y); //outputs 'y = 5'
		x = (int)5.99;
		System.out.println("x = " + x); //outputs 'x = 5'
	}//end main()

}//end class

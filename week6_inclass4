package projectOne;

public class ProjectOne {

	public static void main(String[] args) {
		int counter = 0;
		for (int i = 1; i <= 100; i++) {
			System.out.print(i + "\t");
			counter++;
			if (counter % 10 == 0) {
				System.out.print("\n");
				counter = 0;
			}
		}
	}
}

//Create a 10 x 10 grid of the numbers from 1 to 100

package conditionalOperator;

public class ConditionalOperator {

	public static void main(String[] args) {
		//The conditional operator in Java is the ternary operator from Javascript
		int a = 4;
		int b = 7;
		int max = (a > b)?a:b;
		System.out.println(max); //Prints 7
		
		int studentGrade = 80;
		System.out.println((studentGrade >= 60) ? "Passed" : "Failed"); //Prints "Passed"
	}

}
